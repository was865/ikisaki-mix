<% if (usertabledata != null && usertabledata.department != "*非表示*") { %>

    <tr class="data_item_tr">
          <td class="mobile-hide">
               <% if (usertabledata.department == "イノベーション事業本部") { %>
                    ｲﾉﾍﾞｰｼｮﾝ事業本部
               <% } else { %>
                    <%= usertabledata.department %>
               <% } %>
          </td>
          <td class="table_name" style="font-family: ＭＳ 明朝;"><span class="name"
               <% if (req_user.admin == 1 && usertabledata.err_times >= 5) { %> style="color: red;" <% } %>><%= usertabledata.name %></span>
            <% if (usertabledata.email != null && usertabledata.email != ""){ %>
                <span class="mailto"><a class="sentMail"><i class="far fa-envelope"></i></a><p style="display: none;"><%= usertabledata.email %></p></span><p class="tooltip">送信先：<br><%= usertabledata.email %></p>
            <% } %><span style="display: none;" class="id"><% if (usertabledata.id == "" || usertabledata.id == undefined) { %>tableUser_<%= usertabledata.table_id %><% } else { %><%= usertabledata.id %><% } %></span>
          </td>
        <td class="mobile-hide"><%= usertabledata.information %></td>
        <% switch (usertabledata.status) {
            case '在席': %>
            <td class="find-status"><span class="status_zaiseki icon faa-parent animated-hover"><i class="fa fa-laptop faa-float" aria-hidden="true"></i></span><%= usertabledata.status %></td>
            <% ;break; 
            case '休暇': %>
             <td class="find-status"><span class="status_kyuuka icon faa-parent animated-hover"><i class="fa fa-bed faa-flash" aria-hidden="true"></i></span><%= usertabledata.status %></td>
            <% ;break; 
            case '帰宅': %>
             <td class="find-status"><span class="status_kitaku icon faa-parent animated-hover"><i class="fas fa-home faa-falling"></i></i></span><%= usertabledata.status %></td>
            <% ;break; 
            case '客先': %>
             <td class="find-status"><span class="status_kyakusaki icon faa-parent animated-hover"><i class="fas fa-handshake faa-tada"></i></span><%= usertabledata.status %></td>
            <% ;break; 
            case '社内': 
                if (usertabledata.ikisaki == "応接室１" || usertabledata.ikisaki == "応接室２" || usertabledata.ikisaki == "応接室２" || usertabledata.ikisaki == "営業ＭＴＧ") { %>
                     <td class="find-status"><span class="status_shanai icon faa-parent animated-hover"><i class="fa fa-users faa-tada" aria-hidden="true"></i></span><%= usertabledata.status %></td>
                <% } else if (usertabledata.ikisaki == "サーバ室") { %>
                     <td class="find-status"><span class="status_shanai icon faa-parent animated-hover"><i class="fas fa-server faa-wrench"></i></span><%= usertabledata.status %></td>
                <% } else if (usertabledata.ikisaki == "検証室") { %>
                     <td class="find-status"><span class="status_shanai icon faa-parent animated-hover"><i class="fas fa-wrench faa-wrench"></i></span><%= usertabledata.status %></td>
                <% } else if (usertabledata.ikisaki == "食堂") { %>
                     <td class="find-status"><span class="status_shanai icon faa-parent animated-hover"><i class="fas fa-utensils faa-shake"></i></span><%= usertabledata.status %></td>
                <% } else if (usertabledata.ikisaki == "喫煙室") { %>
                     <td class="find-status"><span class="status_shanai icon faa-parent animated-hover"><i class="fas fa-smoking faa-float"></i></i></span><%= usertabledata.status %></td>
                <% } else if (usertabledata.ikisaki == "リラクゼーション") { %>
                     <td class="find-status"><span class="status_shanai icon faa-parent animated-hover"><i class="fas fa-couch faa-float"></i></span><%= usertabledata.status %></td>
                <% } else if (usertabledata.ikisaki == "社長室") { %>
                     <td class="find-status"><span class="status_shanai icon faa-parent animated-hover"><i class="fas fa-user-secret faa-spin"></i></span><%= usertabledata.status %></td>
                <% } else if (usertabledata.ikisaki == "その他") { %>
                     <td class="find-status"><span class="status_shanai icon faa-parent animated-hover"><i class="fa fa-building" aria-hidden="true"></i></span><%= usertabledata.status %></td>
                <% } else { %>
                     <td class="find-status"><span class="status_shanai icon faa-parent animated-hover"><i class="fa fa-television faa-horizontal" aria-hidden="true"></i></span><%= usertabledata.status %></td>
            <% };break; 
            case '出張': %>
             <td class="find-status"><span class="status_shucchou icon faa-parent animated-hover"><i class="fa fa-suitcase faa-passing" aria-hidden="true"></i></span><%= usertabledata.status %></td>
            <% ;break; 
            case '研修': %>
             <td class="find-status"><span class="status_kennshuu icon faa-parent animated-hover"><i class="fab fa-leanpub faa-shake"></i></span><%= usertabledata.status %></td>
            <% ;break; 
            default: %>
             <td class="find-status"><span class="status_sonota icon faa-parent animated-hover"><i class="fa fa-smile-o faa-spin" aria-hidden="true"></i></span><%= usertabledata.status %></td>
            <% ;break; } %>
        <td class="table_ikisaki"><%= usertabledata.ikisaki %></td>
        <td class="table_time"><%= usertabledata.time %></td>
        <td class="table_memo lengthCheck_memo"><%= usertabledata.memo %></td>
        <td class="mobile-hide"><%= usertabledata.updateTime %></td>
        <td>
            <div class="btn-wrap">
               <% if (usertabledata.id == "" || usertabledata.id == undefined) { %>
                   <a href="/edit/tableUser_<%=  usertabledata.table_id  %>/" class="btn btn-switch"><span>編集</span></a>
               <% } else { %>
                   <a href="/edit/<%=  usertabledata.id  %>/" class="btn btn-switch"><span>編集</span></a>
               <% } %>
            </div>
        </td>
    </tr>

<% } %>